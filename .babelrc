{
	"env": {
		"build": {
			"presets": [
				[
					"env",
					{
						"modules": false,
						"targets": {
							"browsers": [">= 5%"]
						}
					}
				]
			],
			"plugins": ["add-module-exports"]
		},
		"test": {
			"presets": [
				[
					"env",
					{
						"targets": {
							"browsers": [">= 5%"]
						}
					}
				]
			],
			"plugins": ["add-module-exports"]
		}
	}
}
